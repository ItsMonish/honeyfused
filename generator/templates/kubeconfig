- cluster:
    certificate-authority-data: $DEV_CA_DATA
    server: https://dev.internal.net:6443
  name: dev-cluster
- cluster:
    certificate-authority-data: $STAGING_CA_DATA
    server: https://staging.internel.net:6443
  name: staging-cluster
- cluster:
    certificate-authority-data: $PROD_CA_DATA
    server: https://prod.k8s.aws.com:6443
  name: prod-cluster
contexts:
- context:
    cluster: dev-cluster
    user: dev-user
  name: dev-context
- context:
    cluster: staging-cluster
    user: staging-user
  name: staging-context
- context:
    cluster: prod-cluster
    user: prod-user
  name: prod-context
current-context: prod-context
users:
- name: dev-user
  user:
    client-certificate-data: $DEV_CLIENT_CERT
    client-key-data: $DEV_CLIENT_KEY
    token: $DEV_USER_TOKEN
- name: staging-user
  user:
    client-certificate-data: $STAGING_CLIENT_CERT
    client-key-data: $STAGING_CLIENT_KEY
    token: $STAGING_USER_TOKEN
- name: prod-user
  user:
    client-certificate-data: $PROD_CLIENT_CERT
    client-key-data: $PROD_CLIENT_KEY
    token: $PROD_USER_TOKEN
- name: legacy-user
  user:
    client-certificate-data: $PROD_CLIENT_CERT
    client-key-data: $LEGACY_CLIENT_KEY
    token: $LEGACY_USER_TOKEN
